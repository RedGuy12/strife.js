{
	"name": "strife.js",
	"version": "3.1.3",
	"exports": {
		".": {
			"types": "./dist/types/index.d.ts",
			"require": "./dist/cjs/index.js",
			"import": "./dist/esm/index.js",
			"default": "./dist/esm/index.js"
		}
	},
	"files": [
		"dist",
		"src",
		"package.json",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"build": "rm -rf dist && tsc --build tsconfig.cjs.json tsconfig.esm.json tsconfig.types.json && echo {\"type\":\"module\"} > ./dist/esm/package.json",
		"dev": "npm run build && tsc --project tsconfig.esm.json --watch",
		"format": "prettier --write \"**\" \"!dist/**\" --ignore-unknown"
	},
	"prettier": "@redguy12/prettier-config",
	"devDependencies": {
		"@redguy12/prettier-config": "2.0.2",
		"@types/node": "<=18.11.1",
		"discord.js": "14.9.0",
		"prettier": "2.8.0",
		"typescript": "5.1.3"
	},
	"peerDependencies": {
		"discord.js": "*"
	},
	"engines": {
		"node": ">=18.11.1",
		"npm": ">=8.0.0"
	},
	"overrides": {
		"discord-api-types": "0.37.53",
		"@discordjs/builders": "1.6.0",
		"@discordjs/collection": "1.5.0",
		"@discordjs/formatters": "0.3.0",
		"@discordjs/rest": "1.7.0",
		"@discordjs/util": "0.2.0"
	}
}
